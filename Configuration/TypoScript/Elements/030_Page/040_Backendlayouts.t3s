# TYPO3 layout options for content elements
tt_content.stdWrap.innerWrap.cObject = CASE
tt_content.stdWrap.innerWrap.cObject {
    key.field = layout

    0 = TEXT
    0.value = <article class="singleColumn">|</article>

    1 = TEXT
    1.value = <article class="doubleColumn">|</article>

    2 = TEXT
    2.value = <article class="featureSlider">|</article>
}
