config {
    doctype = html5
    absRefPrefix = /

    index_enable = 1
    index_external = 1

    # maybe removed later, because it removes encryption of email adresses
    removeDefaultJS = 1

    uniqueLinkVars = 1

    sys_language_mode = content_fallback
    sys_language_overlay = 1
    sys_language_uid = 0
    htmlTag_langKey = en
    language = en
    locale_all = en_EN.UTF-8

    prefixLocalAnchors = all
    tx_realurl_enable = 1
}
