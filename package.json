{
  "name": "fid-aac",
  "version": "0.0.1",
  "description": "Template for FID AAC",
  "private": true,
  "scripts": {
    "testLocal": "./node_modules/.bin/casperjs  --ssl-protocol=any --ignore-ssl-errors=true test Tests/test-*.js --url='http://fidaac.dev'",
    "testDev": "./node_modules/.bin/casperjs --ssl-protocol=any --ignore-ssl-errors=true test Tests/test-*.js --url='http://aacdev.sub.uni-goettingen.de'",
    "babel": "./node_modules/.bin/babel Resources/Public/JavaScript/concatenated.js -o Resources/Public/JavaScript/babel.js",
    "lintScss": "stylelint 'Resources/Private/Scss/*.scss'",
    "precommit": "npm run lintScss",
    "lintJs": "./node_modules/.bin/eslint Resources/Private/JavaScript/*.Js",
    "createMainCss": "./node_modules/.bin/node-sass Resources/Private/Scss/App.scss | ./node_modules/.bin/postcss --use autoprefixer --no-map -b 'last 2 versions' --use cssnano > Resources/Public/Css/minified.css",
    "createPrintCss": "./node_modules/.bin/node-sass Resources/Private/Scss/Print.scss | ./node_modules/.bin/postcss --use autoprefixer --no-map -b 'last 2 versions' --use cssnano > Resources/Public/Css/print.css",
    "createCss": "npm run createMainCss; npm run createPrintCss",
    "devMainCss": "./node_modules/.bin/node-sass --source-map true Resources/Private/Scss/App.scss Resources/Public/Css/minified.css; ./node_modules/.bin/postcss --use autoprefixer -b 'last 2 versions' Resources/Public/Css/minified.css -o Resources/Public/Css/minified.css",
    "devPrintCss": "./node_modules/.bin/node-sass --output-style expanded --source-comments --source-map-embed Resources/Private/Scss/Print.scss | ./node_modules/.bin/postcss --use autoprefixer -b 'last 2 versions' > Resources/Public/Css/print.css",
    "devCss": "npm run devMainCss; npm run devPrintCss",
    "uglifyjs": "./node_modules/.bin/uglifyjs Resources/Public/JavaScript/babel.js -o Resources/Public/JavaScript/minified.js",
    "buildAll": "npm run lintJs; npm run createCss; node build.js; npm run babel; npm run uglifyjs",
    "devAll": "npm run lintJs; npm run devCss; node build.js; npm run babel; npm run uglifyjs",
    "watchAll": "nodemon -e scss,Js -x \"npm run devAll\"",
    "browsersync": "browser-sync start -c bs-config.js",
    "devBrowser": "concurrently --kill-others --prefix \"[{name}]\" --names \"SCSS & JS,BROWER SYNC\" \"npm run watchAll\" \"npm run browsersync\""
  },
  "repository": "http://github.com/subugoe/typo3-tmpl_fidaac",
  "author": "",
  "license": "AGPL-3.0",
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-cli": "^6.24.1",
    "browser-sync": "^2.18.13",
    "buildify": "^0.4.0",
    "casperjs": "^1.1.3",
    "concurrently": "^3.5.0",
    "cssnano": "^3.10.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.6.1",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-node": "^5.1.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^0.14.3",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "npmrc": "^1.1.1",
    "phantomjs-prebuilt": "^2.1.7",
    "postcss": "^6.0.6",
    "postcss-cli": "^4.1.0",
    "stylelint-config-standard": "^16.0.0",
    "stylelint-order": "^0.5.0",
    "uglify-es": "^3.0.23"
  },
  "dependencies": {
    "jquery": "^3.2.1",
    "lightbox2": "^2.9.0"
  }
}
