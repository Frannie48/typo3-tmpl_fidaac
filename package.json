{
  "name": "fid-aac",
  "version": "0.0.1",
  "description": "Template for FID AAC",
  "private": true,
  "scripts": {
    "testLocal": "./node_modules/.bin/casperjs  --ssl-protocol=any --ignore-ssl-errors=true test Tests/test-*.js --url='http://fidaac.dev'",
    "testDev": "./node_modules/.bin/casperjs --ssl-protocol=any --ignore-ssl-errors=true test Tests/test-*.js --url='http://aacdev.sub.uni-goettingen.de'",
    "babel": "./node_modules/.bin/babel Resources/Public/JavaScript/concatenated.js -o Resources/Public/JavaScript/babel.js",
    "lintJs": "./node_modules/.bin/eslint Resources/Private/JavaScript/*.Js",
    "createCss": "./node_modules/.bin/node-sass Resources/Private/Scss/App.scss Resources/Public/Css/concatenated.css",
    "uglifyjs": "./node_modules/.bin/uglifyjs Resources/Public/JavaScript/babel.js -o Resources/Public/JavaScript/minified.js",
    "buildAll": "npm run lintJs; npm run createCss; node build.js; npm run babel; npm run uglifyjs",
    "watchAll": "nodemon -e scss,Js -x \"npm run buildAll\""
  },
  "repository": "http://github.com/subugoe/typo3-tmpl_fidaac",
  "author": "",
  "license": "AGPL-3.0",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "buildify": "^0.4.0",
    "casperjs": "^1.1.3",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.6.1",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-node": "^5.1.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-standard": "^3.0.1",
    "node-sass": "^4.5.0",
    "nodemon": "^1.11.0",
    "npmrc": "^1.1.1",
    "phantomjs-prebuilt": "^2.1.7",
    "stylelint-config-standard": "^16.0.0",
    "stylelint-order": "^0.5.0",
    "uglify-es": "^3.0.23"
  },
  "dependencies": {
    "jquery": "^3.1.1"
  }
}
